{
  "name": "saved-properties-ui",
  "version": "1.0.0",
  "private": true,
  "description": "An Argonaut MFE",
  "scripts": {
    "publish-to-dev": "argonaut publish",
    "deploy": "argonaut publish --production | argonaut release --production",
    "build": "argonaut build",
    "dev": "argonaut dev",
    "prepare": "argonaut prepare",
    "lint": "eslint . --ext .ts,.js,.tsx --max-warnings=0",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}' --ignore-path .prettierignore",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}' --ignore-path .prettierignore",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@argonaut/actions": "^3.15.0",
    "@construct-kit/core": "^4.29.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  }
}
