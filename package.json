{
  "name": "saved-properties-ui",
  "version": "1.0.0",
  "private": true,
  "description": "An Argonaut MFE",
  "scripts": {
    "publish-to-dev": "argonaut publish",
    "deploy": "argonaut publish --production | argonaut release --production",
    "build": "argonaut build",
    "dev": "argonaut dev",
    "prepare": "argonaut prepare",
    "lint": "eslint . --ext .ts,.js,.tsx --max-warnings=0",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}' --ignore-path .prettierignore",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}' --ignore-path .prettierignore",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@argonaut/actions": "^3.15.0",
    "@construct-kit/core": "^4.29.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@argonaut/cli": "^6.11.3",
    "@argonaut/cli-plugin-typescript": "^3.3.0",
    "@realestate.com.au/argonaut": "^2.33.0",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3"
  }
}
